# Sandvine-SVSDE.repo
#
#

[sandvine-svsde]
name=Sandvine - SVSDE (Service Delivery Engine)
# New, sane URL, for Yum versioned Repo:
baseurl=ftp://{{ packages_server }}/centos/{{ release_code_name }}/$releasever/svrepos/$basearch/svsde-{{ sde_version }}
# Packages are from (old URL format):
# - ftp://{{ packages_server }}/images/Linux/CENTOS6-x64/SDE/svsde-LNX-X.XX.XXXX
enabled=1
protect=0
gpgcheck=0
sslverify=0
