# Sandvine-SVSPB.repo
#
#

[sandvine-svspb]
name=Sandvine - SVSPB
# New, sane URL, for Yum versioned Repo:
baseurl=ftp://{{ packages_server }}/centos/{{ release_code_name }}/$releasever/svrepos/$basearch/svspb-{{ spb_version }}
# Packages are from (old URL format):
# - ftp://{{ packages_server }}/images/Linux/CENTOS6-x64/SPB/svspb-X.XX.XXXX
enabled=1
protect=0
gpgcheck=0
sslverify=0
