#################################################################################################################################

# session tracking

diameter_stack_enableSessionTracking=YES
diameter_stack_sessionTimeoutLoopIntervalMS=100
diameter_stack_dropBlockedMessagesOnSessionExpiry=NO
diameter_stack_dropMessageOnSessionCreateFailure=YES
diameter_stack_refreshSessionTimerOnUpdate=YES
diameter_stack_useSessionIdOnSessionUpdate=NO
diameter_stack_maxPerSessionBlockedMessages=8
diameter_stack_maxSessionsToTrack=100000
# The value should of diameter_stack_sessionTimeoutMS be (diameter_stack_requestRetransmissionIntervalMS * (diameter_stack_requestRetransmissionAttempts + 1)) + 1000
diameter_stack_sessionTimeoutMS=7000
diameter_stack_forceBlockedMessageRelease=NO

# message timeouts

diameter_stack_pendingAnswerTimeoutMS=3000

# request retransmission

diameter_stack_requestRetransmissionIntervalMS=3000
diameter_stack_requestRetransmissionAttempts=1
diameter_stack_sendFailedOutgoingRequestsToApp=YES

# message maximums

diameter_stack_maxIncomingQueuedMessages=2000
diameter_stack_maxOutgoingPerPeerMessages=2000
diameter_stack_maxOutgoingPerPeerBlockedMessages=2000
diameter_stack_maxOutgoingPerPeerReadyMessages=4000
diameter_stack_maxOutgoingPendingRequests=16000
diameter_stack_maxOutgoingPendingAnswers=8000

# Increasing this value will have a negative performance impact
diameter_stack_maxMessageSize=16384

# Support DSCP marking for PTS diameter traffic to guarantee QoS
diameter_stack_connectionIpDscp=0

# message aging

# Message aging configuration for incoming/outgoing  messages is disabled by default.
# If it is enabled age threshold value configured for incoming and outgoing
# messages are applied on all incoming/outgoing messages. all threshold values
# are in milliseconds.
diameter_stack_checkIncomingMessageAge=NO
diameter_stack_checkOutgoingMessageAge=NO
diameter_stack_sendStaleOutgoingRequestsToApp=YES
diameter_stack_dropStaleBlockedRequests=NO
diameter_stack_outgoingMaxAgeEarlyThresholdMS=5500
diameter_stack_outgoingMaxAgeThresholdIncrementMS=1000
diameter_stack_incomingMaxAgeEarlyThresholdMS=2000
diameter_stack_incomingMaxAgeThresholdIncrementMS=1000
diameter_stack_dropStaleIncomingRequests=NO
diameter_stack_dropStaleIncomingAnswers=NO

# message rate limiting

diameter_stack_limitInMessageRate=false
diameter_stack_limitOutMessageRate=false
diameter_stack_inMessageRateLimitingIntervalMS=3000
diameter_stack_outMessageRateLimitingIntervalMS=3000
diameter_stack_maxIncomingMessagesPerSecond=5000
diameter_stack_maxOutgoingMessagesPerSecond=5000
diameter_stack_inRateThresholdPercent=99
diameter_stack_outRateThresholdPercent=99

# message parsing

diameter_stack_enableTopLevelMessageValidation=YES
diameter_stack_enableFullMessageValidation=NO
diameter_stack_enableAvpLookupCache=YES
diameter_stack_alignHeaderLength=YES

# tls config

diameter_stack_enableTLSInClientMode=NO
diameter_stack_enableTLSInServerMode=NO
diameter_stack_obeyCapabilitiesExchangeForTLS=NO

# message dropping

diameter_stack_checkForRetransmittedRequests=YES
diameter_stack_dropRetransmittedRequests=NO

# If a Diameter Answer is received for which the corresponding Request cannot
# be found, this configuration determines whether the Answer is dropped or
# sent to the policy engine(This can occur when there are retransmitted
# requests and answers).
# The default of "YES" causes such answers to be dropped.
# CAUTION: some SandScript applications might not behave properly when
# receiving unsolicited Answers.
diameter_stack_dropUnsolicitedAnswers=YES

diameter_stack_dropMissingOriginHost=NO
diameter_stack_dropOutgoingRateTooHigh=YES
diameter_stack_dropFabricatedOnQueueFull=NO

# partial messages

diameter_stack_maxIncomingPartialMessageLength=32768
diameter_stack_moveDataInReadBufferOnSpace=NO

# name resolution

diameter_stack_nameResolutionTimerIntervalSec=2
diameter_stack_resolvePeerHostNameOnReconnect=YES

# answer processing

diameter_stack_attachRequestToAnswer=YES
diameter_stack_attachRequestToErrorAnswer=YES

# failover

diameter_stack_enableClientPeerFailover=YES
diameter_stack_onlyFailbackOnDWA=NO
diameter_stack_alwaysRespondToDWR=YES

# local routing

diameter_stack_enableLocalRoute=NO
diameter_stack_enableDefaultRoute=NO
diameter_stack_loopBackServerListenPort=36963

# connection rates

diameter_stack_limitConnectionRate=YES
diameter_stack_connectionRateLimitingIntervalMS=1000
diameter_stack_maxConnectionsPerSecond=100

# answer retransmission

diameter_stack_maxAnswerRetransmissionAttempts=10

# message capture

diameter_stack_enableCapture=NO
diameter_stack_messagesPerFile=200
diameter_stack_filePrefix="msgs"
diameter_stack_autoDisableAfter=0
diameter_stack_filterLogicalConnective=disjunction
diameter_stack_filtersEnableApplicationId=disable
diameter_stack_filtersApplicationId=0
diameter_stack_filtersEnableCommandCode=disable
diameter_stack_filtersCommandCode=0
diameter_stack_filtersEnableResultCode=disable
diameter_stack_filtersResultCode=0
diameter_stack_filtersEnableRequestType=disable
diameter_stack_filtersRequestType=0
diameter_stack_filtersEnableOriginHost=disable
diameter_stack_filtersOriginHost=""
diameter_stack_filtersEnableSessionId=disable
diameter_stack_filtersSessionId=""
diameter_stack_filtersEnableDropped=disable
diameter_stack_filtersEnableFailedValidation=disable
diameter_stack_filtersEnableRBitSet=disable
diameter_stack_filtersEnableEBitSet=disable
diameter_stack_filtersEnableTBitSet=disable

# policy
# Maximum instances of one AVP supported in policy. 
diameter_stack_policyMaxAvpInstances=64

diameter_stack_useErrorAnswerABNFFromRFC=YES

# back pressure

diameter_stack_messageReceiveThresholdPercent=99

# connection termination

diameter_stack_alwaysRespondToDPR=YES

# dropped message alarms

diameter_stack_alarmOnDroppedUnsolicitedAnswer=NO

# peer config

diameter_stack_maxPeers=128
diameter_stack_maxLocalPeers=128

# realm routing

diameter_stack_roundRobinBalancing=NO
diameter_stack_exceptionTableTimeout=60
diameter_stack_exceptionTableExtendedTimeout=10

# capability exchange
diameter_stack_disregardApplicationIdsFromCEA=NO
diameter_stack_hostIpAddressAvpInCER=""

# debug

diameter_stack_enableSSLLogging=NO
diameter_stack_enableSocketErrorString=NO
diameter_stack_enableCreationFailureLogging=true
diameter_stack_logMessageErrors=NO
diameter_stack_logParserErrors=NO

